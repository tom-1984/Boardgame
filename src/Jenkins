pipeline {
    agent any
     
    tools {
        maven 'Maven1'
        jdk 'jdk17'
        
    }

    stages {
        stage('Get Checkout') {
            steps {
          git branch: 'feature', url: 'https://github.com/jaiswaladi246/Boardgame.git'
            }
        }
        
        stage('Unit Test Case') {
            steps {
               sh 'mvn test'
            }
        }
        
        stage('Build') {
            steps {
                sh 'mvn package'
            }
        }
    }
}
